{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "double": {
      "quote": "\"",
      "alternateQuote": "'",
      "description": "doublequote"
    },
    "single": {
      "quote": "'",
      "alternateQuote": "\"",
      "description": "singlequote"
    },
    "backtick": {
      "quote": "`",
      "alternateQuote": "\"",
      "description": "backtick"
    },
    "linebreak-style": "off"
  }
}
